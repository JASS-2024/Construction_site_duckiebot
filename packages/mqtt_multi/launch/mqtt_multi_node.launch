<launch>
	<arg name="node_name" value="mqtt_multi_node"/>
	<arg name="veh" doc="Name of vehicle. ex: megaman"/>
    <arg name="param_file_name" default="mqtt_multi_node" doc="Specify a param file. ex:megaman"/>
	<arg name="robot_type" default="duckiebot" doc="Type of robot"/>

	<group ns="$(arg veh)">
	    <node pkg="mqtt_multi" type="mqtt_multi_node.py"  name="$(arg node_name)" output="screen" >
			<rosparam command="load" file="$(find mqtt_multi)/config/$(arg node_name)/$(arg param_file_name).yaml"/>
			<param name="robot_type" value="$(arg robot_type)"/>
		</node>
	</group>

</launch>
